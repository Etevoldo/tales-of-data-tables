<form id="searchForm">
  <label for="itemSearchInput">Search item to rerise:</label>
  <input id="itemSearchInput" type="search" list="itemSearch" placeholder="Type item name">
  <button id="itemSearchBtn">Search rerise tree</button>
</form>
<datalist id="itemSearch">
  {% for arteKey, arteProps in items %}
    <option>{{arteKey}}</option>
  {% endfor %}
</datalist>

<div id="notFoundMessage">
  <p>Couldn't find the tree for <span id="searchTerm"></span></p>
</div>

<div id="cy"></div>

